  "private": true,
  "scripts": {
    "test": "jest --silent",
    "test:watch": "jest --watch --silent",
    "test:cov": "jest --coverage --silent",
    "migrate:status": "npx knex migrate:status --cwd src/adapters/driven/rds",
    "migrate:status:production": "npx knex migrate:status --cwd src/adapters/driven/rds --env production",
    "migrate:latest": "npx knex migrate:latest --cwd src/adapters/driven/rds",
    "migrate:latest:production": "npx knex migrate:latest --cwd src/adapters/driven/rds --env production",
    "migrate:rollback": "npx knex migrate:rollback --cwd src/adapters/driven/rds",
    "migrate:rollback:production": "npx knex migrate:rollback --cwd src/adapters/driven/rds --env production",
    "migrate:make": "npx knex migrate:make --cwd src/adapters/driven/rds",
    "deploy": "npx sls deploy",
    "deploy:local": "npx sls deploy --config serverless.local.yml --stage local",
    "remove": "npx sls remove",
    "remove:local": "npx sls remove --config serverless.local.yml --stage local",
    "build": "tsc"
  },
